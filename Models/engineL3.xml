<?xml version="1.0"?>

  <PropertyList> 
    <path>engineL.ac</path>
 <animation>
  <type>select</type>
  <object-name>prop.L</object-name>
  <condition>
   <less-than>
    <property>engines/engine[2]/rpm</property>
    <value>1000</value>
   </less-than>
  </condition>
 </animation>
<animation>
  <type>spin</type>
  <object-name>prop.L</object-name>
    <property>engines/engine[2]/rpm</property>
    <factor>-0.5</factor>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>
<animation>
  <type>spin</type>
  <object-name>prophub.L</object-name>
    <property>engines/engine[2]/rpm</property>
    <factor>-0.5</factor>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>
  <model>
    <name>pdisk</name>
		<nopreview/>
    <path>Aircraft/fw190/Models/pdisk.xml</path>
    <offsets>
   <x-m>-1.6</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
    </offsets>
  </model>	
  </PropertyList>
