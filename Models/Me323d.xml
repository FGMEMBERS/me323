 <animation>
  <type>select</type>
  <object-name>l3000f</object-name>
    <condition>
      <and>
        <equals>
          <property>sim/weight[0]/selected</property>
          <value>Truck</value>
        </equals>
        <not-equals>
          <property>sim/model/Loads/Load[0]/position-norm</property>
          <value>1</value>
        </not-equals>
      </and>
    </condition>
 </animation>
